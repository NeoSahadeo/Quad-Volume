{
	"manifest_version": 2,
	"name": "Quad Volume",
	"description": "A quadratic volume controller for Apple Music",
	"author": "Neo Sahadeo",
	"version": "1.0",
	"icons": {
		"48": "icons/48.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://music.apple.com/*"
			],
			"js": [
				"script.js"
			]
		}
	],
	"browser_specific_settings": {
		"gecko": {
			"id": "quad-volume@neosahadeo",
			"data_collection_permissions": {
				"required": [
					"none"
				]
			}
		}
	}
}
